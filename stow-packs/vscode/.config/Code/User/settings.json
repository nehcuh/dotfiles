{
    "editor.fontFamily": "Fira Code, 'Courier New', monospace",
    "editor.fontSize": 14,
    "editor.lineHeight": 1.6,
    "editor.tabSize": 2,
    "editor.insertSpaces": true,
    "editor.detectIndentation": false,
    "editor.formatOnSave": true,
    "editor.formatOnPaste": true,
    "editor.renderWhitespace": "boundary",
    "editor.minimap.enabled": false,
    "editor.scrollBeyondLastLine": true,
    "editor.smoothScrolling": true,
    "editor.cursorSmoothCaretAnimation": "on",
    "editor.guides.bracketPairs": "active",
    "editor.wordWrap": "on",
    "editor.quickSuggestions": {
        "other": true,
        "comments": false,
        "strings": true
    },
    "editor.suggestSelection": "first",
    "editor.acceptSuggestionOnEnter": "on",
    "editor.snippetSuggestions": "inline",
    "editor.tabCompletion": "on",
    "editor.hover.enabled": true,
    "editor.parameterHints.enabled": true,
    "editor.autoClosingBrackets": "always",
    "editor.autoClosingQuotes": "always",
    "editor.autoSurround": "languageDefined",
    "editor.suggest.insertMode": "replace",
    "editor.fontLigatures": true,
    "editor.fontWeight": "400",
    "editor.letterSpacing": 0.5,
    "workbench.colorTheme": "Default Dark+",
    "workbench.iconTheme": "vs-seti",
    "workbench.startupEditor": "none",
    "workbench.editor.enablePreview": false,
    "workbench.editor.showTabs": true,
    "workbench.editor.tabCloseButton": "right",
    "workbench.editor.tabSizing": "fit",
    "workbench.sideBar.location": "left",
    "workbench.statusBar.visible": true,
    "workbench.activityBar.visible": true,
    "workbench.panel.defaultLocation": "bottom",
    "explorer.confirmDelete": false,
    "explorer.confirmDragAndDrop": false,
    "explorer.compactFolders": false,
    "files.autoSave": "afterDelay",
    "files.autoSaveDelay": 1000,
    "files.trimTrailingWhitespace": true,
    "files.insertFinalNewline": true,
    "files.trimFinalNewlines": true,
    "files.watcherExclude": {
        "**/node_modules/**": true,
        "**/dist/**": true,
        "**/build/**": true
    },
    "search.exclude": {
        "**/node_modules": true,
        "**/bower_components": true,
        "**/*.code-search": true,
        "**/dist": true,
        "**/build": true,
        "**/.git": true,
        "**/.svn": true,
        "**/.hg": true,
        "**/CVS": true,
        "**/.DS_Store": true,
        "**/Thumbs.db": true
    },
    "git.enableSmartCommit": true,
    "git.autofetch": true,
    "git.confirmSync": false,
    "terminal.integrated.fontSize": 14,
    "terminal.integrated.fontFamily": "Fira Code, 'Courier New', monospace",
    "terminal.integrated.lineHeight": 1.6,
    "terminal.integrated.cursorStyle": "line",
    "terminal.integrated.cursorBlinking": true,
    "terminal.integrated.copyOnSelection": true,
    "telemetry.telemetryLevel": "off",
    "update.mode": "none",
    "update.enableWindowsBackgroundUpdates": false,
    "security.workspace.trust.untrustedFiles": "open",
    "window.restoreWindows": "all",
    "window.zoomLevel": 0,
    "window.titleBarStyle": "native",
    "window.menuBarVisibility": "default",
    "zenMode.hideLineNumbers": false,
    "zenMode.hideStatusBar": false,
    "zenMode.fullScreen": false,
    "zenMode.centerLayout": false,
    "zenMode.restore": true,
    "breadcrumbs.enabled": true,
    "breadcrumbs.symbolPath": "on",
    "breadcrumbs.filePath": "on",
    "markdown.preview.fontSize": 14,
    "markdown.preview.lineHeight": 1.6,
    "markdown.preview.fontFamily": "Fira Code, 'Courier New', monospace",
    "[python]": {
        "editor.defaultFormatter": "ms-python.black-formatter",
        "editor.formatOnSave": true,
        "editor.codeActionsOnSave": {
            "source.organizeImports": true
        }
    },
    "python.defaultInterpreterPath": "python3",
    "python.languageServer": "None",
    "basedpyright.analysis.typeCheckingMode": "strict",
    "basedpyright.analysis.autoImportCompletions": true,
    "basedpyright.analysis.autoSearchPaths": true,
    "basedpyright.analysis.diagnosticMode": "workspace",
    "basedpyright.analysis.stubPath": "./typings",
    "basedpyright.analysis.extraPaths": [],
    "python.testing.pytestEnabled": true,
    "python.testing.unittestEnabled": false,
    "python.testing.nosetestsEnabled": false,
    "[rust]": {
        "editor.defaultFormatter": "rust-lang.rust-analyzer",
        "editor.formatOnSave": true,
        "editor.codeActionsOnSave": {
            "source.organizeImports": true
        }
    },
    "rust-analyzer.check.command": "clippy",
    "rust-analyzer.cargo.loadOutDirsFromCheck": true,
    "rust-analyzer.procMacro.enable": true,
    "rust-analyzer.completion.addCallParentheses": true,
    "[toml]": {
        "editor.defaultFormatter": "tamasfe.even-better-toml",
        "editor.formatOnSave": true
    },
    "[markdown]": {
        "editor.formatOnSave": false,
        "editor.wordWrap": "on"
    },
    "[ini]": {
        "editor.insertSpaces": true,
        "editor.tabSize": 4
    },
    "[json]": {
        "editor.formatOnSave": true
    },
    "[yaml]": {
        "editor.formatOnSave": true
    },
    "vim.easymotion": true,
    "vim.incsearch": true,
    "vim.useSystemClipboard": true,
    "vim.useCtrlKeys": true,
    "vim.hlsearch": true,
    "vim.leader": "<space>",
    "vim.insertModeKeyBindings": [
        {
            "before": [
                "j",
                "j"
            ],
            "after": [
                "<Esc>"
            ]
        }
    ],
    "errorLens.enabled": true,
    "errorLens.fontSize": "12",
    "errorLens.fontStyle": "italic",
    "errorLens.margin": "4px"
}